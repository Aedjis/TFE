@model IEnumerable<Tour0Suisse.Model.ViewUser>

@{
    ViewData["Title"] = "AllUser";
}

<h2>AllUser</h2>

<table class="table List table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Pseudo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pseudo)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new {  id=item.IdUser  }) 
            </td>
        </tr>
}
    </tbody>
</table>
